<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/tv_nombreRestaurante"
        android:layout_width="256dp"
        android:layout_height="32dp"
        android:text="messageNotification"
        android:textSize="15dp"
        android:layout_marginStart="20dp"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/img_logoRestaurant"
        app:layout_constraintStart_toEndOf="@+id/img_logoRestaurant"
        app:layout_constraintTop_toTopOf="@+id/img_logoRestaurant" />

    <TextView
        android:id="@+id/tv_descripcionRestaurante"
        android:layout_width="256dp"
        android:layout_height="77dp"
        android:text="tweetNotification"
        android:textSize="15dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/tv_nombreRestaurante"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/tv_nombreRestaurante"
        app:layout_constraintTop_toBottomOf="@+id/tv_nombreRestaurante" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_logoRestaurant"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.197"
        tools:srcCompat="@tools:sample/avatars"
        tools:ignore="MissingClass" />


</androidx.constraintlayout.widget.ConstraintLayout>