<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GRecyclerViewFeed">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_feed"
        android:layout_width="372dp"
        android:layout_height="379dp"
        app:layout_constraintEnd_toEndOf="@+id/top_image"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_image" />

    <ImageView
        android:id="@+id/top_image"
        android:layout_width="305dp"
        android:layout_height="64dp"
        android:src="@drawable/inicio"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:srcCompat="@tools:sample/avatars" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView2"
        android:layout_width="62dp"
        android:layout_height="55dp"
        android:layout_marginEnd="0dp"
        android:src="@drawable/me"
        app:layout_constraintBottom_toBottomOf="@+id/top_image"
        app:layout_constraintEnd_toStartOf="@+id/top_image"
        app:layout_constraintTop_toTopOf="@+id/top_image"
        app:layout_constraintVertical_bias="0.5"
        tools:srcCompat="@tools:sample/avatars" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/img_post"
        android:layout_width="72dp"
        android:layout_height="65dp"
        android:layout_marginStart="280dp"
        android:src="@drawable/icon_post"
        app:layout_constraintBottom_toTopOf="@+id/icon_messages"
        app:layout_constraintStart_toStartOf="@+id/rv_feed"
        app:layout_constraintTop_toBottomOf="@+id/top_image"
        app:layout_constraintVertical_bias="0.9"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/icon_explore"
        android:layout_width="85dp"
        android:layout_height="68dp"
        android:src="@drawable/icon_explorez"
        app:layout_constraintEnd_toEndOf="@+id/rv_feed"
        app:layout_constraintHorizontal_bias="0.344"
        app:layout_constraintStart_toStartOf="@+id/rv_feed"
        app:layout_constraintTop_toBottomOf="@+id/rv_feed"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/icon_home"
        android:layout_width="85dp"
        android:layout_height="68dp"
        android:src="@drawable/icon_home_select"
        app:layout_constraintEnd_toEndOf="@+id/rv_feed"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/rv_feed"
        app:layout_constraintTop_toBottomOf="@+id/rv_feed"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/icon_noti"
        android:layout_width="85dp"
        android:layout_height="68dp"
        android:src="@drawable/icon_notiz"
        app:layout_constraintEnd_toEndOf="@+id/rv_feed"
        app:layout_constraintHorizontal_bias="0.644"
        app:layout_constraintStart_toStartOf="@+id/rv_feed"
        app:layout_constraintTop_toBottomOf="@+id/rv_feed"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/icon_messages"
        android:layout_width="85dp"
        android:layout_height="68dp"
        android:src="@drawable/icon_messagez"
        app:layout_constraintEnd_toEndOf="@+id/rv_feed"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="@+id/rv_feed"
        app:layout_constraintTop_toBottomOf="@+id/rv_feed"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>