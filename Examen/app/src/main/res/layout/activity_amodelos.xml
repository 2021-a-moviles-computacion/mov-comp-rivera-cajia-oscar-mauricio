<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AModelos">

    <TextView
        android:id="@+id/textNombreFab"
        android:layout_width="257dp"
        android:layout_height="27dp"
        android:text="Modelos"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.409"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066" />

    <Button
        android:id="@+id/btn_abrir_crear_modelo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="Crear modelo"
        app:layout_constraintEnd_toEndOf="@+id/textNombreFab"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textNombreFab"
        app:layout_constraintTop_toBottomOf="@+id/textNombreFab" />

    <ListView
        android:id="@+id/ltv_modelos"
        android:layout_width="284dp"
        android:layout_height="312dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="@+id/btn_volver_a_fabricantes"
        app:layout_constraintHorizontal_bias="0.817"
        app:layout_constraintStart_toStartOf="@+id/btn_volver_a_fabricantes"
        app:layout_constraintTop_toBottomOf="@+id/btn_volver_a_fabricantes" />

    <Button
        android:id="@+id/btn_volver_a_fabricantes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Fabricantes"
        app:layout_constraintEnd_toEndOf="@+id/btn_abrir_crear_modelo"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/btn_abrir_crear_modelo"
        app:layout_constraintTop_toBottomOf="@+id/btn_abrir_crear_modelo" />

</androidx.constraintlayout.widget.ConstraintLayout>